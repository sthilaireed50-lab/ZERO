local function TeleportTo(position)
    if not HumanoidRootPart then return end
    
    HumanoidRootPart.CFrame = CFrame.new(position)
end

-- ============================================
-- SISTEMA DE VISUAL
-- ============================================

local function UpdateVisuals()
    -- FullBright
    if RedzHub.FullBright then
        Lighting.Brightness = 2
        Lighting.ClockTime = 14
        Lighting.FogEnd = 100000
        Lighting.GlobalShadows = false
    else
        Lighting.Brightness = 1
        Lighting.ClockTime = 14
        Lighting.FogEnd = 1000
        Lighting.GlobalShadows = true
    end
    
    -- No Fog
    if RedzHub.NoFog then
        Lighting.FogEnd = 100000
    end
end

-- ============================================
-- SISTEMA DE PLAYER
-- ============================================

local function UpdatePlayerStats()
    Humanoid.WalkSpeed = RedzHub.WalkSpeed
    Humanoid.JumpPower = RedzHub.JumpPower
    
    -- Inf Energy
    if RedzHub.InfEnergy then
        if Character:FindFirstChild("Energy") then
            Character.Energy.Value = 100
        end
    end
    
    -- No Clip
    if RedzHub.NoClip then
        RunService.Stepped:Connect(function()
            pcall(function()
                for _, part in pairs(Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end)
        end)
    end
end

-- ============================================
-- SISTEMA ANTI-AFK
-- ============================================

if RedzHub.AntiAFK then
    local VirtualUser = game:GetService("VirtualUser")
    Player.Idled:Connect(function()
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new(0, 0))
    end)
end

-- ============================================
-- INTERFAZ PRINCIPAL REDZ HUB
-- ============================================

local function CreateMainUI()
    local CoreGui = game:GetService("CoreGui")
    
    -- Eliminar UI anterior si existe
    if CoreGui:FindFirstChild("RedzHubMain") then
        CoreGui.RedzHubMain:Destroy()
    end
    
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "RedzHubMain"
    ScreenGui.Parent = CoreGui
    
    -- Marco principal
    local MainFrame = Instance.new("Frame")
    MainFrame.Name = "MainFrame"
    MainFrame.Size = UDim2.new(0, 500, 0, 600)
    MainFrame.Position = UDim2.new(0.5, -250, 0.5, -300)
    MainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
    MainFrame.BorderSizePixel = 0
    MainFrame.Parent = ScreenGui
    
    -- Esquinas redondeadas
    local Corner = Instance.new("UICorner")
    Corner.CornerRadius = UDim.new(0, 12)
    Corner.Parent = MainFrame
    
    -- T칤tulo
    local Title = Instance.new("TextLabel")
    Title.Name = "Title"
    Title.Size = UDim2.new(1, 0, 0, 50)
    Title.BackgroundColor3 = Color3.fromRGB(30, 30, 45)
    Title.TextColor3 = Color3.fromRGB(255, 50, 50)
    Title.Text = "游댠 REDZ HUB v" .. RedzHub.Version
    Title.Font = Enum.Font.GothamBold
    Title.TextSize = 20
    Title.Parent = MainFrame
    
    local TitleCorner = Instance.new("UICorner")
    TitleCorner.CornerRadius = UDim.new(0, 12)
    TitleCorner.Parent = Title
    
    -- Bot칩n cerrar
    local CloseButton = Instance.new("TextButton")
    CloseButton.Name = "CloseButton"
    CloseButton.Size = UDim2.new(0, 40, 0, 40)
    CloseButton.Position = UDim2.new(1, -45, 0, 5)
    CloseButton.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
    CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    CloseButton.Text = "X"
    CloseButton.Font = Enum.Font.GothamBold
    CloseButton.TextSize = 18
    CloseButton.Parent = Title
    
    CloseButton.MouseButton1Click:Connect(function()
        ScreenGui:Destroy()
    end)
    
    -- Contenedor de tabs
    local TabsContainer = Instance.new("Frame")
    TabsContainer.Name = "TabsContainer"
    TabsContainer.Size = UDim2.new(1, 0, 0, 40)
    TabsContainer.Position = UDim2.new(0, 0, 0, 50)
    TabsContainer.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
    TabsContainer.Parent = MainFrame
    
    -- Tabs
    local Tabs = {"Auto Farm", "Player", "Combat", "Visual", "Teleport", "Misc"}
    local CurrentTab = "Auto Farm"
    
    -- Crear botones de tabs
    for i, tabName in ipairs(Tabs) do
        local TabButton = Instance.new("TextButton")
        TabButton.Name = tabName .. "Tab"
        TabButton.Size = UDim2.new(1/#Tabs, 0, 1, 0)
        TabButton.Position = UDim2.new((i-1)/#Tabs, 0, 0, 0)
        TabButton.BackgroundColor3 = tabName == CurrentTab and Color3.fromRGB(255, 50, 50) or Color3.fromRGB(40, 40, 50)
        TabButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TabButton.Text = tabName
        TabButton.Font = Enum.Font.Gotham
        TabButton.TextSize = 12
        TabButton.Parent = TabsContainer
        
        TabButton.MouseButton1Click:Connect(function()
            CurrentTab = tabName
            -- Actualizar colores de todos los tabs
            for _, tName in ipairs(Tabs) do
                local btn = TabsContainer:FindFirstChild(tName .. "Tab")
                if btn then
                    btn.BackgroundColor3 = tName == CurrentTab and Color3.fromRGB(255, 50, 50) or Color3.fromRGB(40, 40, 50)
                end
            end
            UpdateTabContent()
        end)
    end
    
    -- Contenido
    local ContentFrame = Instance.new("Frame")
    ContentFrame.Name = "ContentFrame"
    ContentFrame.Size = UDim2.new(1, 0, 1, -90)
    ContentFrame.Position = UDim2.new(0, 0, 0, 90)
    ContentFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
    ContentFrame.Parent = MainFrame
    
    -- Scrolling frame
    local ScrollFrame = Instance.new("ScrollingFrame")
    ScrollFrame.Name = "ScrollFrame"
    ScrollFrame.Size = UDim2.new(1, -10, 1, -10)
    ScrollFrame.Position = UDim2.new(0, 5, 0, 5)
    ScrollFrame.BackgroundTransparency = 1
    ScrollFrame.ScrollBarThickness = 5
    ScrollFrame.ScrollBarImageColor3 = Color3.fromRGB(255, 50, 50)
    ScrollFrame.CanvasSize = UDim2.new(0, 0, 0, 800)
    ScrollFrame.Parent = ContentFrame
    
    -- Funci칩n para crear elementos
    local elementY = 10
    local function CreateToggle(name, configKey, description)
        local toggleFrame = Instance.new("Frame")
        toggleFrame.Name = name .. "Frame"
        toggleFrame.Size = UDim2.new(1, -20, 0, 35)
        toggleFrame.Position = UDim2.new(0, 10, 0, elementY)
        toggleFrame.BackgroundTransparency = 1
        toggleFrame.Parent = ScrollFrame
        
        local toggleButton = Instance.new("TextButton")
        toggleButton.Name = name .. "Toggle"
        toggleButton.Size = UDim2.new(0, 120, 0, 30)
        toggleButton.Position = UDim2.new(0, 0, 0, 0)
        toggleButton.BackgroundColor3 = RedzHub[configKey] and Color3.fromRGB(0, 200, 0) or Color3.fromRGB(200, 0, 0)
        toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        toggleButton.Text = name .. ": " .. (RedzHub[configKey] and "ON" or "OFF")
        toggleButton.Font = Enum.Font.Gotham
        toggleButton.TextSize = 12
        toggleButton.Parent = toggleFrame
        
        local descLabel = Instance.new("TextLabel")
        descLabel.Name = name .. "Desc"
        descLabel.Size = UDim2.new(0, 340, 0, 30)
        descLabel.Position = UDim2.new(0, 130, 0, 0)
        descLabel.BackgroundTransparency = 1
        descLabel.TextColor3 = Color3.fromRGB(200, 200, 220)
        descLabel.Text = description
        descLabel.Font = Enum.Font.Gotham
        descLabel.TextSize = 11
        descLabel.TextXAlignment = Enum.TextXAlignment.Left
        descLabel.Parent = toggleFrame
        
        toggleButton.MouseButton1Click:Connect(function()
            RedzHub[configKey] = not RedzHub[configKey]
            toggleButton.BackgroundColor3 = RedzHub[configKey] and Color3.fromRGB(0, 200, 0) or Color3.fromRGB(200, 0, 0)
            toggleButton.Text = name .. ": " .. (RedzHub[configKey] and "ON" or "OFF")
            
            -- Ejecutar funciones
            if configKey == "AutoFarm" then
                if RedzHub.AutoFarm then
                    StartAutoFarm()
                else
                    FarmSystem.Farming = false
                    if FarmSystem.Thread then
                        task.cancel(FarmSystem.Thread)
                    end
                end
            elseif configKey == "FullBright" or configKey == "NoFog" then
                UpdateVisuals()
            elseif configKey == "WalkSpeed" or configKey == "JumpPower" or configKey == "InfEnergy" or configKey == "NoClip" then
                UpdatePlayerStats()
            end
        end)
        
        elementY = elementY + 40
    end
    
    local function CreateSlider(name, configKey, minVal, maxVal, description)
        local sliderFrame = Instance.new("Frame")
        sliderFrame.Name = name .. "SliderFrame"
        sliderFrame.Size = UDim2.new(1, -20, 0, 50)
        sliderFrame.Position = UDim2.new(0, 10, 0, elementY)
        sliderFrame.BackgroundTransparency = 1
        sliderFrame.Parent = ScrollFrame
        
        local nameLabel = Instance.new("TextLabel")
        nameLabel.Name = name .. "Label"
        nameLabel.Size = UDim2.new(1, 0, 0, 20)
        nameLabel.Position = UDim2.new(0, 0, 0, 0)
        nameLabel.BackgroundTransparency = 1
        nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        nameLabel.Text = name .. ": " .. RedzHub[configKey]
        nameLabel.Font = Enum.Font.Gotham
        nameLabel.TextSize = 12
        nameLabel.TextXAlignment = Enum.TextXAlignment.Left
        nameLabel.Parent = sliderFrame
        
        local slider = Instance.new("Frame")
        slider.Name = name .. "Slider"
        slider.Size = UDim2.new(1, 0, 0, 20)
        slider.Position = UDim2.new(0, 0, 0, 25)
        slider.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
        slider.BorderSizePixel = 0
        slider.Parent = sliderFrame
        
        local fill = Instance.new("Frame")
        fill.Name = name .. "Fill"
        fill.Size = UDim2.new((RedzHub[configKey] - minVal) / (maxVal - minVal), 0, 1, 0)
        fill.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
        fill.BorderSizePixel = 0
        fill.Parent = slider
        
        -- Interacci칩n
        local dragging = false
        slider.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                dragging = true
            end
        end)
        
        slider.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                dragging = false
            end
        end)
        
        UserInputService.InputChanged:Connect(function(input)
            if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
                local pos = input.Position
                local relative = slider.AbsolutePosition
                local x = math.clamp(pos.X - relative.X, 0, slider.AbsoluteSize.X)
                local percent = x / slider.AbsoluteSize.X
                local value = math.floor(minVal + (maxVal - minVal) * percent)
                
                fill.Size = UDim2.new(percent, 0, 1, 0)
                nameLabel.Text = name .. ": " .. value
                RedzHub[configKey] = value
                
                UpdatePlayerStats()
            end
        end)
        
        elementY = elementY + 60
    end
    
    -- Funci칩n para actualizar contenido
    local function UpdateTabContent()
        -- Limpiar contenido anterior
        for _, child in pairs(ScrollFrame:GetChildren()) do
            if child:IsA("Frame") then
                child:Destroy()
            end
        end
        
        elementY = 10
        
        if CurrentTab == "Auto Farm" then
            CreateToggle("Auto Farm", "AutoFarm", "Farm autom치tico de NPCs")
            CreateToggle("Auto Quest", "AutoQuest", "Completar quests autom치ticamente")
            CreateToggle("Auto Boss", "AutoBoss", "Farmear bosses autom치ticamente")
            CreateToggle("Auto Sea Beast", "AutoSeaBeast", "Farmear Sea Beasts autom치ticamente")
            CreateToggle("Auto Collect", "AutoCollect", "Recoger frutas y cofres autom치ticamente")
            CreateToggle("Auto Sell", "AutoSell", "Vender items autom치ticamente")
            CreateSlider("Farm Distance", "FarmDistance", 10, 200, "Distancia m치xima para farmear")
            
        elseif CurrentTab == "Player" then
            CreateSlider("Walk Speed", "WalkSpeed", 16, 500, "Velocidad de movimiento")
            CreateSlider("Jump Power", "JumpPower", 50, 500, "Altura del salto")
            CreateSlider("Fly Speed", "FlySpeed", 50, 500, "Velocidad de vuelo")
            CreateToggle("Inf Energy", "InfEnergy", "Energ칤a infinita")
            CreateToggle("No Clip", "NoClip", "Atravesar paredes y objetos")
            CreateToggle("God Mode", "GodMode", "Modo invencible (experimental)")
            
        elseif CurrentTab == "Combat" then
            CreateToggle("Auto Click", "AutoClick", "Click autom치tico cada 0.1s")
            CreateToggle("Auto Skill", "AutoSkill", "Usar habilidades autom치ticamente")
            CreateToggle("Fast Attack", "FastAttack", "Ataque r치pido sin delay")
            CreateToggle("Silent Aim", "SilentAim", "Aim autom치tico (silencioso)")
            CreateToggle("Aim Assist", "AimAssist", "Asistencia de aim")
            CreateToggle("No Cooldown", "NoCooldown", "Sin cooldown en habilidades")
            
        elseif CurrentTab == "Visual" then
            CreateToggle("Full Bright", "FullBright", "Iluminaci칩n m치xima en el mapa")
            CreateToggle("No Fog", "NoFog", "Remover niebla del mapa")
            CreateToggle("ESP Players", "EspPlayers", "Ver jugadores a trav칠s de paredes")
            CreateToggle("ESP NPCs", "EspNPCs", "Ver NPCs a trav칠s de paredes")
            CreateToggle("ESP Chests", "EspChests", "Ver cofres a trav칠s de paredes")
            CreateToggle("Tracers", "Tracers", "Mostrar l칤neas hacia jugadores")
            
        elseif CurrentTab == "Teleport" then
            -- Botones de teleport a islas
            local islands = {"Starter Island", "Jungle Island", "Pirate Village", "Desert Island", "Snow Island", "Marine Fortress", "Safe Zone"}
            
            for _, island in ipairs(islands) do
                local button = Instance.new("TextButton")
                button.Name = island .. "Button"
                button.Size = UDim2.new(1, -20, 0, 35)
                button.Position = UDim2.new(0, 10, 0, elementY)
                button.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
                button.TextColor3 = Color3.fromRGB(255, 255, 255)
                button.Text = "游늸 " .. island
                button.Font = Enum.Font.Gotham
                button.TextSize = 12
                button.Parent = ScrollFrame
                
                button.MouseButton1Click:Connect(function()
                    TeleportTo(TeleportSystem.Islands[island])
                end)
                
                elementY = elementY + 40
            end
            
            -- Separador
            elementY = elementY + 20
            
            -- Teleport a bosses
            local bosses = {"Bobby", "Yeti", "Vice Admiral", "Swan", "Magma Admiral", "Fishman Lord", "Cyborg"}
            
            for _, boss in ipairs(bosses) do
                local button = Instance.new("TextButton")
                button.Name = boss .. "Button"
                button.Size = UDim2.new(1, -20, 0, 35)
                button.Position = UDim2.new(0, 10, 0, elementY)
                button.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
                button.TextColor3 = Color3.fromRGB(255, 255, 255)
                button.Text = "丘덢잺 " .. boss
                button.Font = Enum.Font.Gotham
                button.TextSize = 12
                button.Parent = ScrollFrame
                
                button.MouseButton1Click:Connect(function()
                    TeleportTo(TeleportSystem.Bosses[boss])
                end)
                
                elementY = elementY + 40
            end
            
        elseif CurrentTab == "Misc" then
            CreateToggle("Anti AFK", "AntiAFK", "Evitar desconexi칩n por inactividad")
            CreateToggle("Auto Haki", "AutoHaki", "Activar Haki autom치ticamente")
            CreateToggle("Auto Race", "AutoRace", "Mejorar race autom치ticamente")
            CreateToggle("Auto Buy Fragments", "AutoBuyFragments", "Comprar fragments autom치ticamente")
            CreateToggle("Hide Name", "HideName", "Ocultar tu nombre")
            CreateToggle("Safe Mode", "SafeMode", "Modo seguro anti-ban")
        end
        
        ScrollFrame.CanvasSize = UDim2.new(0, 0, 0, elementY + 20)
    end
    
    -- Inicializar contenido
    UpdateTabContent()
    
    -- Hotkey para abrir/cerrar
    UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if input.KeyCode == Enum.KeyCode.RightControl then
            MainFrame.Visible = not MainFrame.Visible
        end
    end)
    
    return ScreenGui
end

-- ============================================
-- INICIALIZACI칍N
-- ============================================

-- Mostrar warning
ShowWarning()

-- Esperar un momento
task.wait(2)

-- Crear UI principal
local MainUI = CreateMainUI()

-- Actualizar estad칤sticas iniciales
UpdatePlayerStats()
UpdateVisuals()

