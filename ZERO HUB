-- BLOX FRUITS ZERO HUB - VERSI√ìN ORIGINAL
-- Creado completamente desde cero para Delta Executor

local Players = game:GetService("Players")
local Workspace = game:GetService("Workspace")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local VirtualInput = game:GetService("VirtualInputManager")
local Lighting = game:GetService("Lighting")
local UserInputService = game:GetService("UserInputService")

-- Variables principales
local Player = Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
local Humanoid = Character:WaitForChild("Humanoid")

-- Sistema de configuraci√≥n ZERO HUB
local ZeroHub = {
    -- Estado del men√∫
    MenuOpen = true,
    
    -- Sistema de Auto Farm
    AutoFarm = {
        Enabled = false,
        Distance = 30,
        Target = "Bandit",
        Priority = "Nearest",
        AutoCollect = true,
        AutoQuest = false
    },
    
    -- Sistema de Player
    Player = {
        WalkSpeed = 50,
        JumpPower = 50,
        InfEnergy = false,
        InfHealth = false,
        NoClip = false,
        Fly = false,
        FlySpeed = 100
    },
    
    -- Sistema de Combat
    Combat = {
        AutoClick = false,
        AutoSkill = false,
        GodMode = false,
        ClickDelay = 0.1,
        SkillRotation = {"Z", "X", "C"},
        DamageMultiplier = 1
    },
    
    -- Sistema de Visual
    Visual = {
        FullBright = false,
        NoFog = false,
        EspPlayers = false,
        EspNPCs = false,
        EspChests = false,
        Tracers = false,
        Chams = false
    },
    
    -- Sistema de Misc
    Misc = {
        AntiAFK = true,
        AutoSell = false,
        HideName = false,
        AntiStun = false,
        SafeMode = true
    },
    
    -- Sistema de Teleport
    Teleport = {
        SelectedIsland = "Starter Island",
        SelectedBoss = "Bandit Leader"
    }
}

-- Listas de datos ORIGINALES
ZeroHub.Islands = {
    "Starter Island",
    "Jungle Island", 
    "Pirate Village",
    "Desert Island",
    "Snow Island",
    "Marine Fortress",
    "Sky Island",
    "Prison"
}

