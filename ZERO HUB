    AutoClick = false,
    AutoSkills = false,
    GodMode = false,
    NoCooldown = false,
    
    -- Misc
    AutoCollect = true,
    AutoDodge = true,
    AntiAFK = true,
    HideName = false,
    
    -- Visual
    FullBright = false,
    NoFog = false,
    
    -- Teleports
    SelectedIsland = "Starter Island"
}

-- Listas de datos
