-- TAB 4: TELEPORTS
-- ============================================
local TeleportTab = Window:MakeTab({
    Name = "üìç TELEPORTS",
    Icon = "rbxassetid://7072716643",
    PremiumOnly = false
})

TeleportTab:AddDropdown({
    Name = "üèùÔ∏è Seleccionar Isla",
    Default = "Starter Island",
    Options = Islands,
    Callback = function(Value)
        Config.SelectedIsland = Value
    end
})

TeleportTab:AddButton({
    Name = "üöÄ Teleport a Isla",
    Callback = function()
        local locations = {
            ["Starter Island"] = Vector3.new(100, 50, 100),
            ["Jungle Island"] = Vector3.new(-1500, 60, 700),
            ["Pirate Village"] = Vector3.new(-1100, 50, 3900),
            ["Desert Island"] = Vector3.new(1000, 50, 2000),
            ["Snow Island"] = Vector3.new(1100, 150, -1200),
            ["Marine Fortress"] = Vector3.new(-4800, 20, 2800),
            ["Sky Island"] = Vector3.new(4600, 1400, -800),
            ["Prison"] = Vector3.new(4800, 10, -800)
        }
        
        local pos = locations[Config.SelectedIsland]
        if pos then
            HumanoidRootPart.CFrame = CFrame.new(pos)
            OrionLib:MakeNotification({
                Name = "Teleport",
                Content = "Teleport a " .. Config.SelectedIsland,
                Time = 3
            })
        end
    end
})

TeleportTab:AddSection("Teleports R√°pidos")

TeleportTab:AddButton({
    Name = "üí∞ Safe Zone",
    Callback = function()
        HumanoidRootPart.CFrame = CFrame.new(0, 50, 0)
    end
})

TeleportTab:AddButton({
    Name = "üè¥‚Äç‚ò†Ô∏è Pirate Starter",
    Callback = function()
        HumanoidRootPart.CFrame = CFrame.new(-1100, 50, 3900)
    end
})

TeleportTab:AddButton({
    Name = "üëÆ Marine Starter",
    Callback = function()
        HumanoidRootPart.CFrame = CFrame.new(-4800, 20, 2800)
    end
})

TeleportTab:AddButton({
