-- ============================================
-- TAB 1: AUTO FARM
-- ============================================
local FarmTab = Window:MakeTab({
    Name = "‚öîÔ∏è AUTO FARM",
    Icon = "rbxassetid://7072706620",
    PremiumOnly = false
})

FarmTab:AddToggle({
    Name = "üéØ Activar Auto Farm",
    Default = false,
    Callback = function(Value)
        Config.AutoFarm = Value
        OrionLib:MakeNotification({
            Name = "Auto Farm",
            Content = Value and "Activado" or "Desactivado",
            Time = 3
        })
    end
})

FarmTab:AddDropdown({
    Name = "üéØ Seleccionar Boss",
    Default = "Sea Beast",
    Options = Bosses,
    Callback = function(Value)
        Config.SelectedBoss = Value
    end
})

