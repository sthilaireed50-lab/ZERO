TeleportTab:AddButton({
    Name = "ü¶à Sea Beast Zone",
    Callback = function()
        HumanoidRootPart.CFrame = CFrame.new(-2000, 100, 500)
    end
})

-- ============================================
-- TAB 5: VISUAL
-- ============================================
local VisualTab = Window:MakeTab({
    Name = "üëÅÔ∏è VISUAL",
    Icon = "rbxassetid://7072708273",
    PremiumOnly = false
})

VisualTab:AddToggle({
    Name = "üí° Full Bright",
    Default = false,
    Callback = function(Value)
        Config.FullBright = Value
        if Value then
            Lighting.Brightness = 2
            Lighting.ClockTime = 14
            Lighting.FogEnd = 100000
            Lighting.GlobalShadows = false
        else
            Lighting.Brightness = 1
            Lighting.ClockTime = 14
            Lighting.FogEnd = 1000
            Lighting.GlobalShadows = true
        end
    end
})

VisualTab:AddToggle({
    Name = "üå´Ô∏è No Fog",
    Default = false,
    Callback = function(Value)
        Config.NoFog = Value
        if Value then
            Lighting.FogEnd = 100000
        else
            Lighting.FogEnd = 1000
        end
    end
})

VisualTab:AddToggle({
    Name = "üåà Mejorar Gr√°ficos",
    Default = false,
    Callback = function(Value)
        if Value then
            settings().Rendering.QualityLevel = "Level21"
            OrionLib:MakeNotification({
                Name = "Gr√°ficos",
                Content = "Gr√°ficos mejorados",
                Time = 3
            })
        end
    end
})

VisualTab:AddButton({
    Name = "üé® Quitar Efectos",
    Callback = function()
        for _, effect in pairs(Lighting:GetChildren()) do
            if effect:IsA("BlurEffect") or effect:IsA("ColorCorrectionEffect") then
                effect:Destroy()
            end
        end
        OrionLib:MakeNotification({
            Name = "Efectos",
            Content = "Efectos visuales removidos",
            Time = 3
        })
    end
})

