    "Control",
    "Gravity"
}

-- ============================================
-- SISTEMA DE AUTO FARM ORIGINAL
-- ============================================

local FarmThread
local function StartFarmSystem()
    if FarmThread then
        task.cancel(FarmThread)
    end
    
    FarmThread = task.spawn(function()
        while ZeroHub.AutoFarm.Enabled do
            task.wait(0.5)
            
            pcall(function()
                -- Buscar NPCs
                local target = nil
                local closestDistance = math.huge
                
                -- Buscar en Enemies
                for _, npc in pairs(Workspace.Enemies:GetChildren()) do
                    if npc:FindFirstChild("Humanoid") and npc.Humanoid.Health > 0 then
                        if npc:FindFirstChild("HumanoidRootPart") then
                            local distance = (HumanoidRootPart.Position - npc.HumanoidRootPart.Position).Magnitude
                            
                            if distance < closestDistance and distance < ZeroHub.AutoFarm.Distance then
                                closestDistance = distance
                                target = npc
                            end
                        end
                    end
                end
                
                -- Buscar en NPCs
                for _, npc in pairs(Workspace.NPCs:GetChildren()) do
                    if npc:FindFirstChild("Humanoid") and npc.Humanoid.Health > 0 then
                        if npc:FindFirstChild("HumanoidRootPart") then
                            local distance = (HumanoidRootPart.Position - npc.HumanoidRootPart.Position).Magnitude
                            
                            if distance < closestDistance and distance < ZeroHub.AutoFarm.Distance then
                                closestDistance = distance
                                target = npc
                            end
                        end
                    end
                end
                
                -- Si encontramos target
                if target and target:FindFirstChild("HumanoidRootPart") then
                    -- Teleport a posici√≥n √≥ptima
                    local optimalPos = target.HumanoidRootPart.Position + Vector3.new(0, 5, -10)
                    HumanoidRootPart.CFrame = CFrame.new(optimalPos)
                    
                    -- Atacar
                    if ZeroHub.Combat.AutoClick then
                        VirtualInput:SendMouseButtonEvent(0, 0, 0, true, game, 0)
                        task.wait(0.05)
                        VirtualInput:SendMouseButtonEvent(0, 0, 0, false, game, 0)
                    end
                    
                    -- Usar habilidades
                    if ZeroHub.Combat.AutoSkill then
                        for _, skill in ipairs(ZeroHub.Combat.SkillRotation) do
                            VirtualInput:SendKeyEvent(true, skill, false, game)
                            task.wait(0.1)
                            VirtualInput:SendKeyEvent(false, skill, false, game)
                            task.wait(0.3)
                        end
                    end
                    
                    -- Auto colectar drops
                    if ZeroHub.AutoFarm.AutoCollect then
                        for _, drop in pairs(Workspace:GetChildren()) do
                            if drop.Name:find("Fruit") or drop.Name:find("Chest") then
                                if drop:FindFirstChild("Handle") then
                                    local dropDistance = (HumanoidRootPart.Position - drop.Handle.Position).Magnitude
                                    if dropDistance < 50 then
                                        HumanoidRootPart.CFrame = drop.Handle.CFrame
                                    end
                                end
                            end
                        end
                    end
                end
            end)
        end
    end)
end

-- ============================================
-- SISTEMA DE NO CLIP ORIGINAL
-- ============================================

local NoClipConnection
local function UpdateNoClip()
    if NoClipConnection then
        NoClipConnection:Disconnect()
    end
    
    if ZeroHub.Player.NoClip then
        NoClipConnection = RunService.Stepped:Connect(function()
            pcall(function()
                for _, part in pairs(Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end)
        end)
    end
end

-- ============================================
-- SISTEMA DE FLY ORIGINAL
-- ============================================

local FlyConnection
local function UpdateFly()
    if FlyConnection then
        FlyConnection:Disconnect()
        HumanoidRootPart.Anchored = false
    end
    
    if ZeroHub.Player.Fly then
        HumanoidRootPart.Anchored = true
        
        FlyConnection = RunService.Heartbeat:Connect(function()
            pcall(function()
                local camera = Workspace.CurrentCamera
                local moveVector = Vector3.new(0, 0, 0)
                
                -- Controles de vuelo
                if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                    moveVector = moveVector + camera.CFrame.LookVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                    moveVector = moveVector - camera.CFrame.LookVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                    moveVector = moveVector - camera.CFrame.RightVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                    moveVector = moveVector + camera.CFrame.RightVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                    moveVector = moveVector + Vector3.new(0, 1, 0)
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                    moveVector = moveVector - Vector3.new(0, 1, 0)
                end
                
                if moveVector.Magnitude > 0 then
                    HumanoidRootPart.CFrame = HumanoidRootPart.CFrame + (moveVector.Unit * ZeroHub.Player.FlySpeed * 0.1)
                end
            end)
        end)
    end
end

-- ============================================
-- SISTEMA VISUAL ORIGINAL
-- ============================================

local function UpdateVisuals()
    -- FullBright
    if ZeroHub.Visual.FullBright then
        Lighting.Brightness = 2
        Lighting.ClockTime = 14
        Lighting.FogEnd = 100000
        Lighting.GlobalShadows = false
    else
        Lighting.Brightness = 1
        Lighting.ClockTime = 14
        Lighting.FogEnd = 1000
        Lighting.GlobalShadows = true
    end
    
    -- No Fog
    if ZeroHub.Visual.NoFog then
        Lighting.FogEnd = 100000
    end
end

-- ============================================
-- SISTEMA ANTI-AFK ORIGINAL
-- ============================================

if ZeroHub.Misc.AntiAFK then
    local VirtualUser = game:GetService("VirtualUser")
    Player.Idled:Connect(function()
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new(0, 0))
    end)
end

