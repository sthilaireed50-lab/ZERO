-- ============================================
-- SISTEMAS AUTOM√ÅTICOS
-- ============================================

-- Auto Click System
task.spawn(function()
    while task.wait(0.1) do
        if Config.AutoClick then
            VirtualInput:SendMouseButtonEvent(0, 0, 0, true, game, 0)
            task.wait(0.01)
            VirtualInput:SendMouseButtonEvent(0, 0, 0, false, game, 0)
        end
    end
end)

-- Auto Skills System
task.spawn(function()
    while task.wait(1) do
        if Config.AutoSkills then
            if Config.UseSkillZ then
                VirtualInput:SendKeyEvent(true, "Z", false, game)
                task.wait(0.1)
                VirtualInput:SendKeyEvent(false, "Z", false, game)
            end
            
            task.wait(0.5)
            
            if Config.UseSkillX then
                VirtualInput:SendKeyEvent(true, "X", false, game)
                task.wait(0.1)
                VirtualInput:SendKeyEvent(false, "X", false, game)
            end
            
            task.wait(0.5)
            
            if Config.UseSkillC then
                VirtualInput:SendKeyEvent(true, "C", false, game)
                task.wait(0.1)
                VirtualInput:SendKeyEvent(false, "C", false, game)
            end
        end
    end
end)

-- Auto Collect System
task.spawn(function()
    while task.wait(2) do
        if Config.AutoCollect then
            pcall(function()
                for _, item in pairs(Workspace:GetChildren()) do
                    if item.Name:find("Fruit") or item.Name:find("Chest") then
                        if item:FindFirstChild("Handle") then
                            local distance = (HumanoidRootPart.Position - item.Handle.Position).Magnitude
                            if distance < 50 then
                                HumanoidRootPart.CFrame = item.Handle.CFrame
                            end
                        end
                    end
                end
            end)
        end
    end
end)

-- Inicializar
OrionLib:Init()

-- Mensaje de bienvenida
OrionLib:MakeNotification({
    Name = "üéÆ Blox Fruits Hub",
    Content = "Menu cargado exitosamente!",
    Time = 5
})

