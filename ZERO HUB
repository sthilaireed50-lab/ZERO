local Humanoid = Character:WaitForChild("Humanoid")

-- Configuraci√≥n global
local Config = {
    -- Auto Farm
    AutoFarm = false,
    AutoFarmBoss = false,
    AutoFarmSeaBeast = false,
    AutoFarmQuest = false,
    
    -- Player
    WalkSpeed = 50,
    JumpPower = 50,
    InfEnergy = false,
    InfHealth = false,
    
