-- Contenedor de tabs
local TabsContainer = Instance.new("Frame")
TabsContainer.Name = "TabsContainer"
TabsContainer.Size = UDim2.new(1, 0, 0, 40)
TabsContainer.Position = UDim2.new(0, 0, 0, 50)
TabsContainer.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
TabsContainer.BorderSizePixel = 0
TabsContainer.Parent = MainContainer

-- Crear tabs
local Tabs = {"Auto Farm", "Player", "Combat", "Visual", "Teleport", "Misc"}
local CurrentTab = "Auto Farm"

local function CreateTabButton(name, index)
    local button = Instance.new("TextButton")
    button.Name = name .. "Tab"
    button.Size = UDim2.new(1/#Tabs, 0, 1, 0)
    button.Position = UDim2.new((index-1)/#Tabs, 0, 0, 0)
    button.BackgroundColor3 = name == CurrentTab and Color3.fromRGB(0, 100, 200) or Color3.fromRGB(40, 40, 50)
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.Text = name
    button.Font = Enum.Font.Gotham
    button.TextSize = 12
    button.Parent = TabsContainer
    
    button.MouseButton1Click:Connect(function()
        CurrentTab = name
        -- Actualizar todos los botones
        for _, tabName in ipairs(Tabs) do
            local tabBtn = TabsContainer:FindFirstChild(tabName .. "Tab")
            if tabBtn then
                tabBtn.BackgroundColor3 = tabName == CurrentTab and Color3.fromRGB(0, 100, 200) or Color3.fromRGB(40, 40, 50)
            end
        end
        UpdateContent()
    end)
end

-- Crear todos los tabs
for i, tabName in ipairs(Tabs) do
    CreateTabButton(tabName, i)
end

-- Contenido principal
local ContentFrame = Instance.new("Frame")
ContentFrame.Name = "ContentFrame"
ContentFrame.Size = UDim2.new(1, 0, 1, -90)
ContentFrame.Position = UDim2.new(0, 0, 0, 90)
ContentFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 35)
ContentFrame.BorderSizePixel = 0
ContentFrame.Parent = MainContainer

-- Scrolling frame para contenido
local ScrollFrame = Instance.new("ScrollingFrame")
ScrollFrame.Name = "ScrollFrame"
ScrollFrame.Size = UDim2.new(1, -10, 1, -10)
ScrollFrame.Position = UDim2.new(0, 5, 0, 5)
ScrollFrame.BackgroundTransparency = 1
ScrollFrame.ScrollBarThickness = 5
ScrollFrame.ScrollBarImageColor3 = Color3.fromRGB(0, 150, 255)
ScrollFrame.CanvasSize = UDim2.new(0, 0, 0, 800)
ScrollFrame.Parent = ContentFrame

-- Funci√≥n para crear elementos de UI
local elementY = 10
local function CreateToggle(name, configPath, description)
    local toggleFrame = Instance.new("Frame")
    toggleFrame.Name = name .. "Frame"
    toggleFrame.Size = UDim2.new(1, -20, 0, 35)
    toggleFrame.Position = UDim2.new(0, 10, 0, elementY)
    toggleFrame.BackgroundTransparency = 1
    toggleFrame.Parent = ScrollFrame
    
    local toggleButton = Instance.new("TextButton")
    toggleButton.Name = name .. "Toggle"
    toggleButton.Size = UDim2.new(0, 120, 0, 30)
    toggleButton.Position = UDim2.new(0, 0, 0, 0)
    
    -- Obtener valor actual
    local currentValue = ZeroHub
    for key in configPath:gmatch("[^%.]+") do
        currentValue = currentValue[key]
    end
    
    toggleButton.BackgroundColor3 = currentValue and Color3.fromRGB(0, 180, 0) or Color3.fromRGB(180, 0, 0)
    toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    toggleButton.Text = name .. ": " .. (currentValue and "ON" or "OFF")
    toggleButton.Font = Enum.Font.Gotham
    toggleButton.TextSize = 12
    toggleButton.Parent = toggleFrame
    
    local descLabel = Instance.new("TextLabel")
    descLabel.Name = name .. "Desc"
    descLabel.Size = UDim2.new(0, 280, 0, 30)
    descLabel.Position = UDim2.new(0, 130, 0, 0)
    descLabel.BackgroundTransparency = 1
    descLabel.TextColor3 = Color3.fromRGB(200, 200, 220)
    descLabel.Text = description
    descLabel.Font = Enum.Font.Gotham
    descLabel.TextSize = 11
    descLabel.TextXAlignment = Enum.TextXAlignment.Left
    descLabel.Parent = toggleFrame
    
    toggleButton.MouseButton1Click:Connect(function()
        -- Actualizar valor
        local target = ZeroHub
        local keys = {}
        for key in configPath:gmatch("[^%.]+") do
            table.insert(keys, key)
        end
        
        for i = 1, #keys - 1 do
            target = target[keys[i]]
        end
        
        target[keys[#keys]] = not target[keys[#keys]]
        
        -- Actualizar bot√≥n
        toggleButton.BackgroundColor3 = target[keys[#keys]] and Color3.fromRGB(0, 180, 0) or Color3.fromRGB(180, 0, 0)
        toggleButton.Text = name .. ": " .. (target[keys[#keys]] and "ON" or "OFF")
        
        -- Ejecutar funciones
        if configPath == "AutoFarm.Enabled" then
            if ZeroHub.AutoFarm.Enabled then
                StartFarmSystem()
            else
                if FarmThread then
                    task.cancel(FarmThread)
                end
            end
        elseif configPath == "Player.NoClip" then
            UpdateNoClip()
        elseif configPath == "Player.Fly" then
            UpdateFly()
        elseif configPath == "Visual.FullBright" or configPath == "Visual.NoFog" then
            UpdateVisuals()
        elseif configPath == "Combat.AutoClick" then
            -- Sistema de auto click
            if ZeroHub.Combat.AutoClick then
                task.spawn(function()
                    while ZeroHub.Combat.AutoClick do
                        VirtualInput:SendMouseButtonEvent(0, 0, 0, true, game, 0)
                        task.wait(ZeroHub.Combat.ClickDelay)
                        VirtualInput:SendMouseButtonEvent(0, 0, 0, false, game, 0)
                        task.wait(ZeroHub.Combat.ClickDelay)
                    end
                end)
            end
        end
    end)
    
    elementY = elementY + 40
end

local function CreateSlider(name, configPath, min, max, description)
    local sliderFrame = Instance.new("Frame")
    sliderFrame.Name = name .. "SliderFrame"
    sliderFrame.Size = UDim2.new(1, -20, 0, 50)
    sliderFrame.Position = UDim2.new(0, 10, 0, elementY)
    sliderFrame.BackgroundTransparency = 1
    sliderFrame.Parent = ScrollFrame
    
    local nameLabel = Instance.new("TextLabel")
    nameLabel.Name = name .. "Label"
    nameLabel.Size = UDim2.new(1, 0, 0, 20)
    nameLabel.Position = UDim2.new(0, 0, 0, 0)
    nameLabel.BackgroundTransparency = 1
    nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    nameLabel.Text = name
    nameLabel.Font = Enum.Font.Gotham
    nameLabel.TextSize = 12
    nameLabel.TextXAlignment = Enum.TextXAlignment.Left
    nameLabel.Parent = sliderFrame
    
    local slider = Instance.new("Frame")
    slider.Name = name .. "Slider"
    slider.Size = UDim2.new(1, 0, 0, 20)
    slider.Position = UDim2.new(0, 0, 0, 25)
    slider.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
    slider.BorderSizePixel = 0
    slider.Parent = sliderFrame
    
    local fill = Instance.new("Frame")
    fill.Name = name .. "Fill"
    
    -- Obtener valor actual
    local currentValue = ZeroHub
    for key in configPath:gmatch("[^%.]+") do
        currentValue = currentValue[key]
    end
    
    local percentage = (currentValue - min) / (max - min)
    fill.Size = UDim2.new(percentage, 0, 1, 0)
    fill.Position = UDim2.new(0, 0, 0, 0)
    fill.BackgroundColor3 = Color3.fromRGB(0, 150, 255)
    fill.BorderSizePixel = 0
    fill.Parent = slider
    
    local valueLabel = Instance.new("TextLabel")
    valueLabel.Name = name .. "Value"
    valueLabel.Size = UDim2.new(0, 60, 0, 20)
    valueLabel.Position = UDim2.new(1, -60, 0, 25)
    valueLabel.BackgroundTransparency = 1
    valueLabel.TextColor3 = Color3.fromRGB(0, 200, 255)
    valueLabel.Text = tostring(currentValue)
    valueLabel.Font = Enum.Font.GothamBold
    valueLabel.TextSize = 12
    valueLabel.Parent = sliderFrame
    
    -- Configurar interacci√≥n
    local dragging = false
    slider.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
        end
    end)
    
    slider.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local pos = Vector2.new(input.Position.X, input.Position.Y)
            local relative = slider.AbsolutePosition
            local x = math.clamp(pos.X - relative.X, 0, slider.AbsoluteSize.X)
            local percentage = x / slider.AbsoluteSize.X
            local value = math.floor(min + (max - min) * percentage)
            
            fill.Size = UDim2.new(percentage, 0, 1, 0)
            valueLabel.Text = tostring(value)
            
            -- Actualizar configuraci√≥n
            local target = ZeroHub
            local keys = {}
            for key in configPath:gmatch("[^%.]+") do
                table.insert(keys, key)
            end
            
            for i = 1, #keys - 1 do
                target = target[keys[i]]
            end
            
            target[keys[#keys]] = value
            
            -- Actualizar en tiempo real
            if configPath == "Player.WalkSpeed" then
                Humanoid.WalkSpeed = value
            elseif configPath == "Player.JumpPower" then
                Humanoid.JumpPower = value
            end
        end
    end)
    
    elementY = elementY + 60
end

-- Funci√≥n para actualizar contenido seg√∫n tab
local function UpdateContent()
    -- Limpiar contenido anterior
    for _, child in pairs(ScrollFrame:GetChildren()) do
        if child:IsA("Frame") then
            child:Destroy()
        end
    end
    
    elementY = 10
    
    if CurrentTab == "Auto Farm" then
        CreateToggle("Auto Farm", "AutoFarm.Enabled", "Farm autom√°tico de NPCs")
        CreateToggle("Auto Collect", "AutoFarm.AutoCollect", "Recoger frutas y cofres autom√°ticamente")
        CreateSlider("Farm Distance", "AutoFarm.Distance", 10, 100, "Distancia m√°xima para farmear")
        
    elseif CurrentTab == "Player" then
        CreateSlider("Walk Speed", "Player.WalkSpeed", 16, 300, "Velocidad de movimiento")
        CreateSlider("Jump Power", "Player.JumpPower", 50, 300, "Altura del salto")
        CreateSlider("Fly Speed", "Player.FlySpeed", 50, 300, "Velocidad de vuelo")
        CreateToggle("No Clip", "Player.NoClip", "Atravesar paredes y objetos")
        CreateToggle("Fly Mode", "Player.Fly", "Modo de vuelo (WASD + Space/Shift)")
        
    elseif CurrentTab == "Combat" then
        CreateToggle("Auto Click", "Combat.AutoClick", "Click autom√°tico cada 0.1s")
        CreateToggle("Auto Skill", "Combat.AutoSkill", "Usar habilidades autom√°ticamente")
        CreateToggle("God Mode", "Combat.GodMode", "Modo invencible (experimental)")
        CreateSlider("Click Delay", "Combat.ClickDelay", 0.05, 1, "Delay entre clicks")
        
    elseif CurrentTab == "Visual" then
        CreateToggle("Full Bright", "Visual.FullBright", "Iluminaci√≥n m√°xima en el mapa")
        CreateToggle("No Fog", "Visual.NoFog", "Remover niebla del mapa")
        CreateToggle("ESP Players", "Visual.EspPlayers", "Ver jugadores a trav√©s de paredes")
        CreateToggle("ESP NPCs", "Visual.EspNPCs", "Ver NPCs a trav√©s de paredes")
        CreateToggle("ESP Chests", "Visual.EspChests", "Ver cofres a trav√©s de paredes")
        
    elseif CurrentTab == "Teleport" then
        -- Crear botones de teleport
        local teleports = {
            {"Safe Zone", Vector3.new(0, 50, 0)},
            {"Starter Island", Vector3.new(100, 50, 100)},
            {"Jungle Island", Vector3.new(-1500, 60, 700)},
            {"Pirate Village", Vector3.new(-1100, 50, 3900)},
            {"Desert Island", Vector3.new(1000, 50, 2000)},
            {"Snow Island", Vector3.new(1100, 150, -1200)}
        }
        
        for i, teleport in ipairs(teleports) do
            local button = Instance.new("TextButton")
            button.Name = teleport[1] .. "Button"
            button.Size = UDim2.new(1, -20, 0, 35)
            button.Position = UDim2.new(0, 10, 0, elementY)
            button.BackgroundColor3 = Color3.fromRGB(0, 100, 200)
            button.TextColor3 = Color3.fromRGB(255, 255, 255)
            button.Text = "üìç " .. teleport[1]
            button.Font = Enum.Font.Gotham
            button.TextSize = 12
            button.Parent = ScrollFrame
            
            button.MouseButton1Click:Connect(function()
                HumanoidRootPart.CFrame = CFrame.new(teleport[2])
            end)
            
            elementY = elementY + 40
        end
        
    elseif CurrentTab == "Misc" then
        CreateToggle("Anti AFK", "Misc.AntiAFK", "Evitar desconexi√≥n por inactividad")
        CreateToggle("Auto Sell", "Misc.AutoSell", "Vender items autom√°ticamente")
        CreateToggle("Hide Name", "Misc.HideName", "Ocultar tu nombre")
        CreateToggle("Anti Stun", "Misc.AntiStun", "No recibir aturdimiento")
        CreateToggle("Safe Mode", "Misc.SafeMode", "Modo seguro anti-ban")
    end
    
    -- Ajustar tama√±o del canvas
    ScrollFrame.CanvasSize = UDim2.new(0, 0, 0, elementY + 10)
end

-- Inicializar contenido
UpdateContent()

-- Configurar WalkSpeed y JumpPower iniciales
Humanoid.WalkSpeed = ZeroHub.Player.WalkSpeed
Humanoid.JumpPower = ZeroHub.Player.JumpPower

-- Hotkey para abrir/cerrar men√∫
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if input.KeyCode == Enum.KeyCode.RightShift then
        MainContainer.Visible = not MainContainer.Visible
    end
end)

